<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.byeKiloh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MenuA">
        <activity android:name=".activitys.E_Crud" />
        <activity android:name=".activitys.A_Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.B_Registro"
            android:parentActivityName=".activitys.A_Login" />
        <activity android:name=".activitys.C_Privacidad" />
        <activity
            android:name=".activitys.D_Main"
            android:parentActivityName=".activitys.A_Login"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EjercicioActivity"
            android:parentActivityName=".activitys.D_Main" />
    </application>

</manifest>