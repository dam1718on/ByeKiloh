<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.byeKiloh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MenuA">
        <activity android:name=".Activity_e_CRUD"></activity>
        <activity android:name=".Activity_a_Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_b_Registro"
            android:parentActivityName=".Activity_a_Login" />
        <activity android:name=".Activity_c_Privacidad" />
        <activity
            android:name=".Activity_d_Main"
            android:parentActivityName=".Activity_a_Login"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EjercicioActivity"
            android:parentActivityName=".Activity_d_Main" />
    </application>

</manifest>